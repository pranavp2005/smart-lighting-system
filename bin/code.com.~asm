.MODEL TINY

.DATA
PORTA equ 0000h
PORTB equ 0002h
PORTC equ 0004h
CWREG equ 0006h

.CODE
.STARTUP
ORG 0000h

; Read from port A, and output the values to port B.
MOV al, 10010000b
OUT CWREG, al

start:  IN al, PORTA
OUT PORTB, al
JMP start

END



